<!DOCTYPE html>
<html>
<head>
  <title><%= @title %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    
    <h1><%= @title %> </h1>
  </header>
  <div id='main-container'>
    <h1>This is the header</h1>
    <%= yield %>
  </div>
</body>
</html>
